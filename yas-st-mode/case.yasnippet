# -*- mode: snippet -*-
# name: ST CASE statement
# key: case
# --
CASE ${1:condition} OF
    ${2:label-1}:
        ${0:(* statements *)}
    ${3:label-2, label-3}:
        ${0:(* statements *)}
    ${4:label-4 .. label-5}:
        ${0:(* statements *)}
    ELSE
        ${0:(* statements *)}
END_CASE;
